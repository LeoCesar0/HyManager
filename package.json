{
  "name": "hymanager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "map-pdf": "tsc scripts/pdfMapping/index.ts --outdir ./dist || true && node dist/scripts/pdfMapping/index.js",
    "test:pos": "NODE_ENV=test jest --runInBand",
    "test:pre": "NODE_ENV=test next dev",
    "test": "npx concurrently \"pnpm test:pre\" \"pnpm test:pos\"",
    "test:inspect": "NODE_ENV=test node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:debug": "NODE_ENV=test npx jest -- --watch",
    "test:current": "NODE_ENV=test node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --testNamePattern=#current --watch",
    "test:current:run": "NODE_ENV=test jest --testNamePattern=#current --detectOpenHandles --forceExit"
  },
